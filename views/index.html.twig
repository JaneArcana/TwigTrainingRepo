<!doctype html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">
    <title>Twig - {{ title }}</title>
</head>
<body>
<div role="main">
    {% block index %}
        <h2>
            <a href="/index.php">Index</a>
        </h2>
    {% endblock index %}

    <pre style="overflow: auto;line-height: 1.3em;font-size: 14px;color: #fff;background-color: #232125;padding: 0.7em;">
        {% spaceless %}
            {% block code %}
                Twig Certification Train Repo
            {% endblock code %}
        {% endspaceless %}
    </pre>

    <div style="overflow: auto;line-height: 1.3em;font-size: 14px;color: #000000;background-color: #efefef;padding: 0.7em;">
        {% block sample %}
            <div style="">
                {% for k, sample in samples %}
                    <div style="width: 24%; float: left;">
                        <p>
                            <b>{{ k|replace({'./views/': ''}) }}</b>
                        </p>
                        {% if sample is iterable %}
                            {% for k, item in sample %}
                                <p style="margin-left:20px;">
                                    <a href="/index.php?path={{ item['path'] }}">
                                        {{ item['file'] }}
                                    </a>
                                </p>
                            {% endfor %}
                        {% endif %}
                    </div>
                {% endfor %}
            </div>
        {% endblock sample %}
    </div>
</div>
<footer>

</footer>

<script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="js/libs/jquery-1.7.1.min.js"><\/script>')</script>
</body>
</html>

